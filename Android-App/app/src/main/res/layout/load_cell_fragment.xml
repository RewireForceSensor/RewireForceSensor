    <TextView
        android:id="@+id/loadPad0"
        android:layout_width="68dp"
        android:layout_height="46dp"
        android:background="#FFDBDB"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/loadCellView"
        app:layout_constraintEnd_toEndOf="@+id/loadCellView"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="@+id/loadCellView"
        app:layout_constraintTop_toTopOf="@+id/loadCellView"
        app:layout_constraintVertical_bias="0.048" />

    <com.example.rewirebluetoothforcesensor.LoadCellView
        android:id="@+id/loadCellView"
        android:layout_width="185dp"
        android:layout_height="420dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.492" />

    <ImageView
        android:id="@+id/copLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/loadCellView"
        app:layout_constraintEnd_toEndOf="@+id/loadCellView"
        app:layout_constraintStart_toStartOf="@+id/loadCellView"
        app:layout_constraintTop_toTopOf="@+id/loadCellView"
        app:srcCompat="@android:drawable/presence_invisible" />

    <ImageView
        android:id="@+id/copRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/loadCellView2"
        app:layout_constraintEnd_toEndOf="@+id/loadCellView2"
        app:layout_constraintStart_toStartOf="@+id/loadCellView2"
        app:layout_constraintTop_toTopOf="@+id/loadCellView2"
        app:srcCompat="@android:drawable/presence_invisible" />

