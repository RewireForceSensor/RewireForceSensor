<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/fragment_height"
    android:background="@android:color/transparent">


    <ImageView
        android:id="@+id/rightFoot"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="@+id/leftFoot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.233"
        app:layout_constraintStart_toEndOf="@+id/leftFoot"
        app:layout_constraintTop_toTopOf="@+id/leftFoot"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/_75px_right_shoe_sole" />

    <ImageView
        android:id="@+id/leftFoot"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_75px_left_shoe_sole" />

    <TextView
        android:id="@+id/leftTotalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/pad0"
        app:layout_constraintEnd_toEndOf="@+id/padLtot"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/padLtot"
        app:layout_constraintTop_toBottomOf="@+id/padLtot"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/rightTotalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/leftTotalText"
        app:layout_constraintEnd_toEndOf="@+id/padRtot"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/padRtot"
        app:layout_constraintTop_toTopOf="@+id/leftTotalText"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/weightlabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Weight (lbs)"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/leftFoot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/pad0"
        android:layout_width="115dp"
        android:layout_height="41dp"
        android:background="#1AC31313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/leftFoot"
        app:layout_constraintEnd_toEndOf="@+id/padLtot"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/padLtot"
        app:layout_constraintTop_toBottomOf="@+id/padLtot" />

    <TextView
        android:id="@+id/pad1"
        android:layout_width="68dp"
        android:layout_height="46dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginBottom="64dp"
        android:background="#1AC31313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/padLtot"
        app:layout_constraintEnd_toStartOf="@+id/pad2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/leftFoot"
        app:layout_constraintTop_toTopOf="@+id/leftFoot"
        app:layout_constraintVertical_bias="1.1" />

    <TextView
        android:id="@+id/pad2"
        android:layout_width="68dp"
        android:layout_height="46dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:background="#1AC31313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/pad1"
        app:layout_constraintEnd_toEndOf="@+id/leftFoot"
        app:layout_constraintTop_toTopOf="@+id/pad1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/pad3"
        android:layout_width="115dp"
        android:layout_height="41dp"
        android:background="#1A48C313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/pad0"
        app:layout_constraintEnd_toEndOf="@+id/padRtot"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/padRtot"
        app:layout_constraintTop_toTopOf="@+id/pad0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/pad4"
        android:layout_width="68dp"
        android:layout_height="46dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#1A48C313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/pad5"
        app:layout_constraintEnd_toEndOf="@+id/rightFoot"
        app:layout_constraintTop_toTopOf="@+id/pad5"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/pad5"
        android:layout_width="68dp"
        android:layout_height="46dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="#1A4EF606"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/pad2"
        app:layout_constraintEnd_toStartOf="@+id/pad4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rightFoot"
        app:layout_constraintTop_toTopOf="@+id/pad2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/padLtot"
        android:layout_width="115dp"
        android:layout_height="41dp"
        android:background="#1AC31313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/leftFoot"
        app:layout_constraintEnd_toEndOf="@+id/leftFoot"
        app:layout_constraintHorizontal_bias="0.261"
        app:layout_constraintStart_toStartOf="@+id/leftFoot"
        app:layout_constraintTop_toTopOf="@+id/leftFoot"
        app:layout_constraintVertical_bias="0.501" />

    <TextView
        android:id="@+id/padRtot"
        android:layout_width="115dp"
        android:layout_height="41dp"
        android:background="#1A48C313"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/padLtot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.673"
        app:layout_constraintStart_toEndOf="@+id/leftFoot"
        app:layout_constraintTop_toTopOf="@+id/padLtot"
        app:layout_constraintVertical_bias="0.0" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="278dp"
        android:layout_height="30dp"
        android:progressBackgroundTint="#FF4EF606"
        android:progressTint="#E4ADAF"
        android:progressTintMode="src_atop"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/leftFoot"
        app:layout_constraintVertical_bias="0.703" />

</androidx.constraintlayout.widget.ConstraintLayout>